        <TextBox x:Name="results" HorizontalAlignment="Left" Height="119" Margin="10,140,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="272"/>
        <Button Content="Dif" HorizontalAlignment="Left" Margin="47,115,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button Content="Less" HorizontalAlignment="Left" Margin="127,115,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
